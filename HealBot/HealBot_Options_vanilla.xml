<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

    <Slider name="HealBot_Options_SliderTemplate" inherits="HorizontalSliderTemplate" virtual="true">
        <Size>
          <AbsDimension x="240" y="16" />
        </Size>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentText" inherits="GameFontNormal" parentKey="Text">
					<Anchors>
						<Anchor point="TOP" relativePoint="TOP" x="0" y="12"/>
					</Anchors>
				</FontString>
				<FontString name="$parentLow" inherits="GameFontNormal" parentKey="Text">
					<Anchors>
						<Anchor point="LEFT" relativePoint="LEFT" x="8" y="-15"/>
					</Anchors>
				</FontString>
				<FontString name="$parentHigh" inherits="GameFontNormal" parentKey="Text">
					<Anchors>
						<Anchor point="RIGHT" relativePoint="RIGHT" x="-8" y="-15"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
    </Slider>

</Ui>
