<Bindings>
	<Binding name="HEALBOTTOGGLEOPTIONS" header="HBBUTTONS" category="BINDING_CATEGORY_HEALBOT">
        if keystate == "down" then
            HealBot_Options_ShowHide()
        end
    </Binding>
	<Binding name="HEALBOTTOGGLEENABLED" category="BINDING_CATEGORY_HEALBOT">
        if keystate == "down" then
            HealBot_ToggleHealBot()
        end
    </Binding>
</Bindings>
