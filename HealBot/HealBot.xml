      <AbsDimension y="18"/>
    </Size>
    <Layers>
    <Layer level="ARTWORK">
      <FontString name="$parent_text" inherits="GameFontNormalSmall" />
      <FontString name="$parent_text2" inherits="GameFontNormalSmall" />
    </Layer>
