  <Frame name="HealBot" parent="UIParent" frameStrata="LOW" toplevel="true" inherits="SecureFrameTemplate">
    <Scripts>
      <OnLoad>
        HealBot_OnLoad(self);
      </OnLoad>
      <OnEvent>
        HealBot_OnEvent(self,event,...);
      </OnEvent>
      <OnUpdate>
        HealBot_OnUpdate(self, elapsed);
      </OnUpdate>
    </Scripts>
  </Frame>
    
